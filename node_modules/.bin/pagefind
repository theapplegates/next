#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/thor3/Documents/nextra5/nextra-nextra-5.0.0-alpha.24/node_modules/.pnpm/pagefind@1.3.0/node_modules/pagefind/lib/runner/node_modules:/Users/thor3/Documents/nextra5/nextra-nextra-5.0.0-alpha.24/node_modules/.pnpm/pagefind@1.3.0/node_modules/pagefind/lib/node_modules:/Users/thor3/Documents/nextra5/nextra-nextra-5.0.0-alpha.24/node_modules/.pnpm/pagefind@1.3.0/node_modules/pagefind/node_modules:/Users/thor3/Documents/nextra5/nextra-nextra-5.0.0-alpha.24/node_modules/.pnpm/pagefind@1.3.0/node_modules:/Users/thor3/Documents/nextra5/nextra-nextra-5.0.0-alpha.24/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/thor3/Documents/nextra5/nextra-nextra-5.0.0-alpha.24/node_modules/.pnpm/pagefind@1.3.0/node_modules/pagefind/lib/runner/node_modules:/Users/thor3/Documents/nextra5/nextra-nextra-5.0.0-alpha.24/node_modules/.pnpm/pagefind@1.3.0/node_modules/pagefind/lib/node_modules:/Users/thor3/Documents/nextra5/nextra-nextra-5.0.0-alpha.24/node_modules/.pnpm/pagefind@1.3.0/node_modules/pagefind/node_modules:/Users/thor3/Documents/nextra5/nextra-nextra-5.0.0-alpha.24/node_modules/.pnpm/pagefind@1.3.0/node_modules:/Users/thor3/Documents/nextra5/nextra-nextra-5.0.0-alpha.24/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../pagefind/lib/runner/bin.cjs" "$@"
else
  exec node  "$basedir/../pagefind/lib/runner/bin.cjs" "$@"
fi
